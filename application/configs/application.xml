<?xml version="1.0" encoding="UTF-8"?>
<application xmlns:zf="http://framework.zend.com/xml/zend-config-xml/1.0/">
  <default>
    <system>
      <hostname>www.example.com</hostname>
      <base-url>/</base-url>
      <routes>
        <default>
          <type value="Zend_Controller_Router_Route_Static" />
          <route value="" />
          <defaults>
            <module value="user" />
            <controller value="account" />
            <action value="login" />
          </defaults>
        </default>
      </routes>
    </system>
    <general>
      <locale>
        <code>fr_FR</code>
        <timezone>Europe/Paris</timezone>
      </locale>
      <database>
        <connection>
          <core_setup>
            <engine></engine>
            <params>
              <host></host>
              <user></user>
              <password></password>
              <dbname></dbname>
            </params>
          </core_setup>
          <core_read>
            <use>core_setup</use>
          </core_read>
          <core_write>
            <use>core_setup</use>
          </core_write>
        </connection>
      </database>
    </general>
    <modules>
      <One_Core>
        <codePool>core</codePool>
        <active>0</active>
        <route type="core/router.route" path="core" name="core" />
      </One_Core>
      <One_Admin_Core>
        <codePool>core</codePool>
        <active>0</active>
        <route type="admin.core/router.route" />
        <requires>
          <One_Core />
        </requires>
      </One_Admin_Core>
      <One_User>
        <codePool>core</codePool>
        <active>0</active>
        <route type="user/router.route" path="user" name="user" />
      </One_User>
      <One_Admin_User>
        <codePool>core</codePool>
        <active>0</active>
        <route type="admin.core/router.route" path="user" />
        <requires>
          <One_User />
          <One_Admin_Core />
        </requires>
      </One_Admin_User>
      <One_Cms>
        <codePool>core</codePool>
        <active>0</active>
        <route type="cms/router.route" name="default" />
      </One_Cms>
      <One_Admin_Cms>
        <codePool>core</codePool>
        <active>0</active>
        <route type="admin.cms/router.route" path="cms" />
      </One_Admin_Cms>
      <One_OpenId>
        <codePool>core</codePool>
        <active>0</active>
        <route type="openid/router.route" />
      </One_OpenId>
    </modules>
    <frontoffice>
      <layout>
        <design>default</design>
        <template>default</template>
      </layout>
    </frontoffice>
  </default>
  <production>
    <system>
      <hostname>one</hostname>
    </system>
    <modules>
      <One_Core>
        <active>1</active>
      </One_Core>
      <One_User>
        <active>1</active>
      </One_User>
      <One_Cms>
        <active>0</active>
      </One_Cms>
    </modules>
  </production>
  <preprod extends="production" />
  <testing extends="production" />
  <debug extends="production" />
</application>